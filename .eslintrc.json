{
  "parser": "babel-eslint",
  "extends": ["airbnb", "prettier"],
  "env": {
    "browser": true,
    "es6": true
  },
  "rules": {
    "react/jsx-filename-extension": ["off"],
    "react/destructuring-assignment": ["off"],
    "react/forbid-prop-types": ["off"]
  },
  "settings": {
    "import/resolve": {
      "moduleDirectory": ["node_modules"]
    }
  }
}
